@model List<CatViewModel>

@{
    ViewData["Title"] = "AllCats";
}

@section Styles
{
    <link href="~/css/cats.css" rel="stylesheet"/>
}


@foreach (var cat in Model)
{
    <partial name="_Cat" model="cat"/>
}

@section Scripts{
    <script>
        document.getElementsByClassName('cats')[0].innerHTML = '<li>Changed by the script</li>'
    </script>
}
